<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CardGameView">
        <attr name="shapeAppearance" />
        <attr name="android:elevation" />
        <attr name="label" format="string" localization="suggested" />
        <attr name="cardType" format="enum">
            <enum name="primary" value="0" />
            <enum name="secondary" value="1" />
            <enum name="back" value="2" />
        </attr>
        <attr name="suit" format="enum">
            <enum name="heard" value="0" />
            <enum name="club" value="1" />
            <enum name="diamond" value="2" />
            <enum name="spade" value="3" />
            <enum name="none" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="UserView">
        <attr name="android:rotation" />
        <attr name="android:name" />
        <attr name="score" format="string" />
        <attr name="userType" format="enum">
            <enum name="primary" value="0" />
            <enum name="secondary" value="1" />
        </attr>
    </declare-styleable>
</resources>